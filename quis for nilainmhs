#include <stdio.h>
#include <stdlib.h>
    int main(void){
        int nilaimhs[5]={90,80,100,85,75};
        int nilaimhs_upg[5];
        int i=0, j=0;
        printf("nilaimhs_upg \n");
        for(i=4;i>=0;i--){
            nilaimhs_upg[i]=nilaimhs[j];
            printf("%d", nilaimhs[j]);
            j++;
            printf("\n");
        }
        return 0;
    }
